<thlogic>
    <attr sel="#social_admin_table">
        <attr sel="tbody" th:remove="all-but-first">
            <attr sel="tr[0]" th:each="feed : ${feed}">
                <attr sel="td.feed_img/img" th:src="${feed.img}"/>
                <attr sel="td.feed_nickname/h5" th:text="${feed.memberIdx}"/>
<!--                <attr sel="td.feed_tags" th:text="${feed.tagNum}"/>-->
                <attr sel="td.feed_tags" th:text="0"/>
<!--                <attr sel="td.feed_likes" th:text="${feed.likeNum}"/>-->
                <attr sel="td.feed_likes" th:text="0"/>
<!--                <attr sel="td.feed_comments" th:text="${feed.commentNum}"/>-->
                <attr sel="td.feed_comments" th:text="0"/>
                <attr sel="td.feed_date/time" th:datetime="${feed.createdAt}" th:text="${#temporals.format(feed.createdAt, 'yyyy-MM-dd')}"/>
            </attr>
        </attr>
    </attr>
</thlogic>